<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphml SYSTEM "http://www.graphdrawing.org/dtds/graphml.dtd">
<graphml>
  <key id="d1" for="edge" attr.name="weight" attr.type="double"/>
  <key id="name" for="node" attr.name="name" attr.type="string"/>
  <key id="link" for="node" attr.name="link" attr.type="string"/>
  <key id="item_id" for="node" attr.name="item_id" attr.type="integer"/>
  <graph id="animalValues-05" edgedefault="undirected" clipContent="false">
<% for node in @distinct_meme_items -%>
    <node id="<%= node.id %>"><data key="link"><%= node.item.link %></data><data key="name"><%= node.item.title %></data><data key="item_id"><%= node.item.id %></data></node>
<% end -%>
<% for edge in @meme.item_relationships -%>
    <edge source="<%= edge.item_id %>" target="<%= edge.related_item_id %>"><data key="d1"><%= 1.0 / (edge.cosine_similarity ** 3) %></data></edge>
<% end -%>
  </graph>
</graphml>
