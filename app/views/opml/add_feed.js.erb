<% if defined?(@feed) && ! @feed.nil? %>
<% if @already_existed == true %>
alert('You already subscribe to that feed.');
<% else %>
li = document.createElement('li');
li.id = "feed_<%= @feed.id %>";
li.innerHTML = "<%= @feed.link %>";
$('list_of_feeds').insertBefore(li, $('list_of_feeds').getElementsByTagName('li')[0]);
<% end %>
<% end %>
