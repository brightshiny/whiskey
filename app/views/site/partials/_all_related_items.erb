<span class="related_items_title">More</span><span class="all_related_items"><% meme.distinct_meme_items(run).sort_by{ |mi| mi.item.published_at }.each do |mi| %><% if mi.item.feed.logo.nil?; mi.item.feed.logo = "/assets/images/fauxvicon.gif"; end; %><span class="article_favicon"><% if primary_item.id == mi.item.id %><%= link_to_item_with_tracking(image_tag(mi.item.feed.logo, :alt => mi.item.feed.title, :size => "16x16", :class => "feed_favicon", :style => "border-color: #D24A31;"), mi.item) %><% elsif (Time.now - mi.item.published_at) < 2.hours %><%= link_to_item_with_tracking(image_tag(mi.item.feed.logo, :alt => mi.item.feed.title, :size => "16x16", :class => "feed_favicon", :style => "border-color: #fc0;"), mi.item) %><% else %><%= link_to_item_with_tracking(image_tag(mi.item.feed.logo, :alt => mi.item.feed.title, :size => "16x16", :class => "feed_favicon"), mi.item) %><% end %></span><% end %></span><span class="details"> <%= link_to "Details &raquo;", :controller => :site, :action => :meme, :id => meme.id %></span>