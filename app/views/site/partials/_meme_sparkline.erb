<% if ! meme.nil? && meme.strength_over_time.size > 1 %>
<h3>Meme Strength Over Time</h3>
<span id="sparkline_<%= meme.id %>" class="sparkline"> </span>
<script type="text/javascript">
$('#sparkline_<%= meme.id %>').sparkline([<%= meme.strength_over_time.join(", ") %>], { type:'line', width:'300px', height:'50px', spotRadius:'4', spotColor:'#fc0', minSpotColor:'#2579d2', maxSpotColor:'#D24A31', lineColor:'#333',fillColor:'#efefef' });
</script>
<div style="padding-bottom: 1.5em;">&nbsp;</div>
<% end %>